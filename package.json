{
  "name": "next-fastapi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "pip3 install -r requirements.txt && uvicorn api.index:app --reload --port 5328",
    "fastapi-prod": "pip3 install -r requirements.txt && uvicorn api.index:app --host 0.0.0.0 --port 5328",
    "next-dev": "next dev",
    "dev": "concurrently \"pnpm run next-dev\" \"pnpm run fastapi-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prod": "next build && concurrently \"pnpm run start\" \"pnpm run fastapi-prod\""
  },
  "dependencies": {
    "@types/node": "20.1.4",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "concurrently": "^8.0.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "next": "13.4.3",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "react-markdown": "^9.0.1"
  },
  "nextConfig": {
    "experimental": {
      "appDir": true
    }
  }
}